{
    "name": "gibmit_iap17a/musicshuffle",
    "description": "App to shuffle music and create playlists",
    "homepage": "https://github.com/hdahlheim/musicshuffle",
    "license": "We don't care",
    "authors": [
        {
            "name": "Malte Henning Frans Dahlheim",
            "email": "malte.dahlheim@gibmit.ch",
            "homepage": "https://dahlheim.ch/"
        },
        {
            "name": "Tatyana Vogel",
            "email": "tatyana.vogel@gibmit.ch"
        }
    ],
    "require": {
        "php": ">=7.1",
        "ext-json": "*",
        "leocavalcante/siler": "^1.7",
        "twig/twig": "^3.0",
        "vlucas/phpdotenv": "^3.6"
    },
    "require-dev": {
    },
    "config": {
        "process-timeout": 0,
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        },
        "files": [
          "functions/pdo.php",
          "functions/validators.php",
          "functions/auth.php"
        ]
    },
    "scripts": {
        "start": "php -S localhost:8080 -t public",
        "test": "phpunit"
    }
}
